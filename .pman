[run]
main_file = "main.py"
auto_build = false
auto_save = true

[general]
name = "Game"
renderer = "basic"
material_mode = "legacy"
physics_engine = "builtin"

[build]
asset_dir = "assets/"
export_dir = "assets/"
ignore_patterns = [ "*.blend1", "*.blend2",]
converters = [ "native2bam", "blend2bam",]
